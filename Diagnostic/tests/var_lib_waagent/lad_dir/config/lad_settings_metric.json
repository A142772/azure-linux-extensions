              },
              "performanceCounters": {
                "performanceCounterConfiguration": [
                  {
                    "class": "Processor",
                    "condition": "IsAggregate=TRUE",
                    "annotation": [
                      {
                        "displayName": "Aggregate CPU %utilization",
